{
  "name": "xtralife-js-client",
  "version": "4.2.3",
  "description": "Simple javascript client for Xtralife gaming backend",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/*.js",
    "build:browser": "parcel build index.js --bundle-node-modules -o bundle.min.js -d lib",
    "build:sample": "parcel build index.js --bundle-node-modules -o bundle.min.js -d sample/js",
    "start": "http-server sample",
    "sample": "parcel build index.js --bundle-node-modules -o bundle.min.js -d sample/js && http-server sample"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clanofthecloud/javascript-client.git"
  },
  "author": "chris@xtralife.cloud",
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "superagent": "^5.3.1"
  },
  "engines": {
    "node": ">= 0.10.26"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "should": "*",
    "parcel-bundler": "^1.12.4"
  },
  "readmeFilename": "Readme.md"
}
